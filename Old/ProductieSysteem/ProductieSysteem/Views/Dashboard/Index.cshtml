@model ProductieSysteem.Models.Dashboard

@{    
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";    
}

<h2>Invoeren</h2>
@Html.ActionLink("Vorige week","")
@Html.ActionLink("Volgende week", "")

<div class="row">
    <div class="col-md-4 col-md-offset-4">
        <p>Huidge week @Html.DisplayFor(m => m.WeekNummer)</p>
    </div>
</div>

<div class="row">
    @foreach (DayOfWeek day in Enum.GetValues(typeof(DayOfWeek)))
    {
        <div class="col-xs-12 col-md-3 panel panel-default">
            <div class="panel-heading">@day</div>
            <div class="panel-body">
                @Html.DropDownList("Dagdelen")
            </div>
        </div>
    }
    </div>
 

@Html.ActionLink("Uitloggen", "LogOut")